<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="nomeGiorno" alignment="TOP_LEFT" promptText="Nome turno">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </TextField>
      <TextField fx:id="fasciaOraria" promptText="Fascia Oraria" />
      <Button fx:id="insert" mnemonicParsing="false" text="Inserisci Turno" />
   </children>
</HBox>
